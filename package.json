{
  "name": "@astrouxds/design-tokens",
  "version": "2.0.0-beta.0",
  "description": "Astro UXDS Design Tokens",
  "main": "index.js",
  "dependencies": {
    "style-dictionary": "^3"
  },
  "scripts": {
    "start": "npm run build",
    "build": "node ./build.js",
    "prepack": "npm run build",
    "transform.dark": "node_modules/.bin/token-transformer data/tokens.json tokens/tokens-dark.json dark --expandTypography --expandShadow --preserveRawValue=true",
    "transform.light": "node_modules/.bin/token-transformer data/tokens.json tokens/tokens.light.json beta,light --expandTypography --expandShadow --preserveRawValue=true",
    "transform": "node_modules/.bin/token-transformer data/tokens.json tokens/tokens.json beta --expandTypography --expandShadow --preserveRawValue=true",
    "make": "npm run transform && npm run transform.light && node preformat.js && npm run build"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RocketCommunicationsInc/astro-design-tokens"
  },
  "author": "Rocket Communications",
  "bugs": {
    "url": "https://github.com/RocketCommunicationsInc/astro-design-tokens/issues"
  },
  "homepage": "https://www.astrouxds.com",
  "devDependencies": {
    "lodash": "^4.17.21",
    "tinycolor2": "^1.4.2",
    "token-transformer": "^0.0.19"
  }
}
