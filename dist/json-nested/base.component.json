{
  "button": {
    "color": {
      "background": {
        "primary": {
          "default": "#4dacff",
          "hover": "#92cbff",
          "disabled": "#4dacff66"
        },
        "secondary": "#ffffff00",
        "borderless": "#ffffff00"
      },
      "text": {
        "primary": "#080c11",
        "secondary": {
          "interactive": {
            "default": "#4dacff",
            "hover": "#92cbff"
          }
        },
        "borderless": {
          "default": "#4dacff",
          "hover": "#92cbff"
        }
      },
      "border": {
        "primary": {
          "default": "#4dacff",
          "hover": "#92cbff",
          "disabled": "#4dacff66"
        },
        "secondary": {
          "default": "#4dacff",
          "hover": "#92cbff"
        },
        "borderless": "#ffffff00"
      }
    },
    "radius": "3px",
    "opacity": {
      "disabled": "40%"
    },
    "icon": {
      "color": {
        "fill": {
          "primary": "#080c11",
          "secondary": {
            "default": "#4dacff",
            "hover": "#92cbff"
          },
          "borderless": {
            "default": "#4dacff",
            "hover": "#92cbff"
          }
        }
      }
    },
    "text": {
      "fontFamily": "'Roboto', -apple-system, BlinkMacSystemFont, 'Segoe UI', Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
      "fontWeight": "400",
      "lineHeight": "calc(24 / 16)",
      "fontSize": "1rem",
      "letterSpacing": "0.005em",
      "paragraphSpacing": 0,
      "textDecoration": "none",
      "textCase": "none"
    },
    "padding": {
      "x": {
        "medium": "0.5rem",
        "medium-iconOnly": "0.5rem"
      },
      "y": {
        "medium": "1rem",
        "medium-iconOnly": "0.5rem"
      }
    }
  },
  "input": {
    "color": {
      "background": {
        "default": "#101923"
      },
      "border": {
        "default": "#2b659b",
        "hover": "#92cbff",
        "error": "#ff3838"
      },
      "text": {
        "default": "#ffffff"
      }
    },
    "padding": {
      "x": {
        "medium": "0.5rem",
        "small": "0.25rem",
        "large": "0.75rem"
      },
      "y": {
        "medium": "0.5rem",
        "small": "0.5rem",
        "large": "0.5rem"
      }
    },
    "text": {
      "fontFamily": "'Roboto', -apple-system, BlinkMacSystemFont, 'Segoe UI', Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
      "fontWeight": "400",
      "lineHeight": "calc(20 / 16)",
      "fontSize": "1rem",
      "letterSpacing": "0.005em",
      "paragraphSpacing": 0,
      "textDecoration": "none",
      "textCase": "none"
    },
    "BorderWidth": "1px",
    "radius": "3px"
  },
  "link": {
    "color": {
      "text": {
        "default": "#4dacff",
        "hover": "#4dacff"
      }
    }
  },
  "card": {
    "color": {
      "border": "#51555b"
    }
  },
  "container": {
    "color": {
      "border": "#000000"
    }
  },
  "classification-banner": {
    "color": {
      "background": {
        "topsecretsci": "#fce83a",
        "topsecret": "#ff8c00",
        "secret": "#c8102e",
        "confidential": "#0033a0",
        "cui": "#502b85",
        "unclassified": "#007a33"
      }
    }
  },
  "status-symbol": {
    "color": {
      "fill": {
        "critical": {
          "on-dark": "#ff3838",
          "on-light": "#ff2a04"
        },
        "serious": {
          "on-dark": "#ffb302",
          "on-light": "#ffaf3d"
        },
        "caution": {
          "on-dark": "#fce83a",
          "on-light": "#fad800"
        },
        "normal": {
          "on-dark": "#56f000",
          "on-light": "#00e200"
        },
        "standby": {
          "on-dark": "#2dccff",
          "on-light": "#64d9ff"
        },
        "off": {
          "on-dark": "#a4abb6",
          "on-light": "#7b8089"
        }
      },
      "border": {
        "critical": "#661102",
        "serious": "#664618",
        "caution": "#645600",
        "normal": "#005a00",
        "standby": "#285766",
        "off": "#3c3e42"
      }
    },
    "borderWidth": {
      "on-dark": 0,
      "on-light": "1px"
    }
  },
  "gsb": {
    "icon": {
      "color": {
        "fill": {
          "default": "#4dacff",
          "hover": "#92cbff"
        }
      }
    },
    "color": {
      "background": "#172635",
      "text": "#ffffff"
    }
  },
  "slider": {
    "radius": {
      "background-track": "1px"
    }
  },
  "checkbox": {
    "radius": "2px"
  },
  "radio": {
    "radius": {
      "inner": "4px",
      "outer": "9px"
    }
  },
  "switch": {
    "radius": {
      "track": "10px"
    }
  },
  "scrollbar": {
    "shadow": {
      "inner": {
        "vertical": "inset 3px 3px 3px 0px rgba(0, 0, 0, 0.5)",
        "horizontal": "inset 1px 3px 3px 0px rgba(0, 0, 0, 0.5)"
      }
    },
    "radius": "4px"
  },
  "progress": {
    "radius": {
      "inner": "8px",
      "outer": "10px"
    }
  },
  "tag": {
    "shadow": {
      "inner": {
        "pass": "inset 0px 0px 5px 0px rgb(86, 240, 0)",
        "fail": "inset 0px 0px 5px 0px rgb(255, 56, 56)",
        "unknown": "inset 0px 0px 5px 0px rgb(164, 171, 182)"
      }
    }
  },
  "indeterminate-progress": {
    "radius": {
      "inner": "23px",
      "outer": "30px"
    }
  },
  "notification-banner": {
    "radius": {
      "inner": "2px",
      "outer": "3px"
    },
    "color": {
      "border": {
        "inner": {
          "normal": "#56f000",
          "caution": "#fce83a",
          "serious": "#ffb302",
          "critical": "#ff3838",
          "standby": "#2dccff",
          "off": "#a4abb6",
          "default": "#4dacff"
        },
        "outer": {
          "normal": "#56f000",
          "caution": "#fce83a",
          "serious": "#ffb302",
          "critical": "#ff3838",
          "standby": "#2dccff",
          "off": "#a4abb6",
          "default": "#4dacff"
        }
      }
    }
  },
  "menu": {
    "divider": {
      "color": {
        "fill": "#51555b"
      }
    }
  },
  "monitoring-icon": {
    "badge": {
      "color": {
        "background": "#101923"
      }
    }
  },
  "clock": {
    "datetime": {
      "color": {
        "background": "#101923"
      }
    }
  },
  "log": {
    "color": {
      "border": "#000000"
    }
  },
  "tooltip": {
    "color": {
      "background": "#3c3e42",
      "text": "#ffffff"
    },
    "radius": "1px"
  },
  "timeline": {
    "header": {
      "color": {
        "background": "#172635"
      }
    },
    "cell": {
      "color": {
        "background": "#1c3f5e"
      }
    }
  }
}